<script src="https://maps.googleapis.com/maps/api/js?key=<%= googleMapsApiKey %>&callback=initMap" 
    async defer></script>

    <script>
        function initMap() {
          // Set the university's location (replace with actual coordinates)
          var universityLocation = { lat: 1.855655342188528, lng: 103.083320064897 };
      
          // Create a map centered on the university's location
          var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 15,
            center: universityLocation
          });
           
          // Add a marker at the university's location
          var marker = new google.maps.Marker({
            position: universityLocation,
            map: map,
            title: "University Location"
          });
        }
      </script>

