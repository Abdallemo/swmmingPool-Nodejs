<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users Feedback Dashboard</title>
    <link
        href="https://cdn.jsdelivr.net/npm/flowbite@2.5.2/dist/flowbite.min.css"
        rel="stylesheet" />
    <link rel="stylesheet" href="/stylesheets/output.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body class="bg-gray-100 min-h-screen flex flex-col">
    <div class="container mx-auto px-4 py-8">
        <h1 class="text-3xl font-bold mb-6 text-center text-gray-800">Users Feedback Dashboard</h1>
        
        <% usersFeedback.forEach(feedback => { %>
        <div class="bg-white rounded-lg shadow-md p-6 mb-4">
            <h2 class="text-xl font-semibold text-gray-700"><%= feedback.name %></h2>
            <p class="text-sm text-gray-500"><%= feedback.email %></p>
            <p class="mt-4 text-gray-600"><%= feedback.feedback %></p>
        </div>
        <% }) %>

        <!-- Navigation Button -->
        <div class="mt-8 flex justify-center">
            <a href="/admin/dashboard"
               class="bg-blue-600 hover:bg-blue-700 text-white font-medium py-2 px-4 rounded-lg shadow-md transition">
               Back to Admin Dashboard
            </a>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/flowbite@2.5.2/dist/flowbite.min.js"></script>
</body>
</html>
